list_a = [('apple', 'banana', 'orange', 'grapes'), ('cricket', 'football', 'hockey'), ('car', 'bicycle', 'bus')]
# Write your code here
n=int(input())

L=[]
for i in range(n):
    index=input().split()
    tuple_index=int(index[0])
    value_index=int(index[1])
    L.append(list_a[tuple_index][value_index])
print(L)
